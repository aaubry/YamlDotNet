    <file src="bin\Release\net35\YamlDotNet.xml" target="lib\net35" />

    <file src="bin\Release\net20\YamlDotNet.dll" target="lib\net20" />
    <file src="bin\Release\net20\YamlDotNet.xml" target="lib\net20" />

    <file src="bin\Release\netstandard1.3\YamlDotNet.dll" target="lib\netstandard1.3" />
